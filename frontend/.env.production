# BICI AI Voice Agent Frontend - Production Environment

# API Configuration (will be set by Render automatically)
VITE_API_URL=${REACT_APP_API_URL}
VITE_WS_URL=${REACT_APP_WS_URL}

# Environment
VITE_APP_ENVIRONMENT=production
VITE_APP_NAME=BICI AI Voice Agent
VITE_APP_VERSION=1.0.0

# Feature Flags for Production
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# UI Configuration
VITE_THEME_MODE=light
VITE_DEFAULT_LANGUAGE=en
VITE_SHOW_PERFORMANCE_METRICS=false

# WebSocket Configuration
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# Cache Configuration
VITE_CACHE_TTL=600000
VITE_ENABLE_OFFLINE_MODE=true

# Security
VITE_ENABLE_CSP=true
VITE_TRUSTED_DOMAINS=bici-ai-backend.onrender.com,bici-ai.yourdomain.com

# Build Configuration
VITE_GENERATE_SOURCEMAP=false
VITE_BUNDLE_ANALYZER=false