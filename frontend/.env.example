# BICI AI Voice Agent Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_WS_URL=ws://localhost:3001

# Environment
VITE_APP_ENVIRONMENT=development
VITE_APP_NAME=BICI AI Voice Agent
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# UI Configuration
VITE_THEME_MODE=light
VITE_DEFAULT_LANGUAGE=en
VITE_SHOW_PERFORMANCE_METRICS=false

# WebSocket Configuration
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# Cache Configuration
VITE_CACHE_TTL=300000
VITE_ENABLE_OFFLINE_MODE=false

# Security
VITE_ENABLE_CSP=true
VITE_TRUSTED_DOMAINS=localhost,127.0.0.1

# Build Configuration
VITE_GENERATE_SOURCEMAP=true
VITE_BUNDLE_ANALYZER=false