{
  "name": "bici-voice-server",
  "version": "1.0.0",
  "description": "BICI Voice Agent Backend Server",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:redis": "tsx scripts/test-redis-comprehensive.ts",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance --testTimeout=120000",
    "test:resilience": "jest --testPathPattern=resilience",
    "test:e2e": "jest --testPathPattern=e2e --testTimeout=90000",
    "test:unit": "jest --testPathPattern=unit",
    "redis:validate": "tsx scripts/redis-validation-suite.ts",
    "redis:test-infrastructure": "tsx src/scripts/test-redis.ts",
    "redis:test-lead-cache": "tsx scripts/test-lead-cache.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "twilio": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "chalk": "^4.1.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "typescript": "^5.3.3"
  }
}
