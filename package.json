{
  "name": "bici-ai-assistant",
  "version": "1.0.0",
  "description": "BICI Bike Store AI Assistant - ElevenLabs Conversational AI System",
  "type": "commonjs",
  "main": "api-server.js",
  "scripts": {
    "start": "node api-server.js",
    "dev": "nodemon api-server.js",
    "setup": "node scripts/setup.js",
    "deploy": "node scripts/deploy.js",
    "test": "jest",
    "lint": "eslint src/ --ext .js",
    "format": "prettier --write src/**/*.js",
    "db:migrate": "node scripts/database-setup.js",
    "validate": "node scripts/validate-config.js",
    "generate-keys": "node scripts/generate-keys.js",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "build:backend": "echo 'Backend build completed - using ES modules'",
    "build:frontend": "cd frontend && npm install && npm run build:production",
    "build:client": "cd client && npm install && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "build:all": "npm run build:backend && npm run build:frontend && npm run build:client",
    "install:frontend": "cd frontend && npm install",
    "install:client": "cd client && npm install",
    "install:all": "npm install && npm run install:frontend && npm run install:client",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:client": "cd client && npm start",
    "preview:frontend": "cd frontend && npm run preview:production",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:client": "cd client && npm run lint || true",
    "lint:all": "npm run lint && npm run lint:frontend && npm run lint:client",
    "clean": "rm -rf node_modules frontend/node_modules client/node_modules frontend/dist client/build",
    "worker": "node src/background/worker.js"
  },
  "keywords": [
    "elevenlabs",
    "twilio",
    "ai",
    "conversational-ai",
    "bike-store",
    "customer-service",
    "voice-assistant"
  ],
  "author": "BICI Bike Store",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "twilio": "^4.19.0",
    "googleapis": "^128.0.0",
    "google-auth-library": "^9.4.1",
    "@upstash/redis": "^1.28.2",
    "@supabase/supabase-js": "^2.39.0",
    "node-fetch": "^3.3.2",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "validator": "^13.11.0",
    "morgan": "^1.10.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "@types/node": "^20.10.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bici-bikes/ai-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/bici-bikes/ai-assistant/issues"
  },
  "homepage": "https://github.com/bici-bikes/ai-assistant#readme"
}